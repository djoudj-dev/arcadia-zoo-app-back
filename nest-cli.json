{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "modules/mail/template/**/*",
        "outDir": "dist/arcadia-zoo-app-back/",
        "watchAssets": true
      },
      {
        "include": "modules/mail/templates/**/*",
        "outDir": "dist/src"
      }
    ],
    "watchAssets": true
  }
}
